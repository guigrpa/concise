    "sequelize": "^4.37.6",
    "sqlite3": "^4.0.0",
    "concise-yaml": "*",
    "concise": "*",
    "concise-json": "*",
    "concise-pg": "*",
    "concise-flow": "*",
    "concise-graphql": "*",
    "concise-diagram": "*",
    "concise-sequelize": "*",
    "concise-firebase": "*"
  }
}
